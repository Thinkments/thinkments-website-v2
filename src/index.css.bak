:root {
  --admin-primary: #1e3a5f;
  --admin-primary-hover: #152d4a;
  --admin-accent: #00b4d8;
  --admin-accent-hover: #0096b8;
  --admin-secondary: #ff6b35;
  --admin-secondary-hover: #e55525;
  --admin-bg: #f8f9fa;
  --admin-card: #fff;
  --admin-border: #e0e4e8;
  --admin-text-primary: #1e3a5f;
  --admin-text-secondary: #6c757d;
  --admin-sidebar-width: 260px;
  --admin-sidebar-collapsed: 80px;
  --admin-header-height: 64px;
}

.admin-layout {
  background-color: var(--admin-bg);
  min-height: 100vh;
}

.admin-sidebar {
  background-color: var(--admin-primary);
  color: #fff;
  width: var(--admin-sidebar-width);
  transition: width .3s;
}

.admin-sidebar.collapsed {
  width: var(--admin-sidebar-collapsed);
}

.admin-header {
  background-color: var(--admin-card);
  border-bottom: 1px solid var(--admin-border);
  height: var(--admin-header-height);
}

.admin-card {
  background-color: var(--admin-card);
  border: 1px solid var(--admin-border);
  border-radius: 8px;
  box-shadow: 0 1px 3px #0000000f;
}

.admin-btn-primary {
  background-color: var(--admin-accent);
  color: #fff;
  border-radius: 8px;
  padding: .5rem 1rem;
  transition: background-color .2s;
}

.admin-btn-primary:hover {
  background-color: var(--admin-accent-hover);
}

.admin-btn-secondary {
  background-color: var(--admin-secondary);
  color: #fff;
  border-radius: 8px;
  padding: .5rem 1rem;
  transition: background-color .2s;
}

.admin-btn-secondary:hover {
  background-color: var(--admin-secondary-hover);
}

.admin-stat-card {
  background: linear-gradient(135deg, var(--admin-primary) 0%, var(--admin-accent) 100%);
  color: #fff;
  border-radius: 8px;
  padding: 1.5rem;
}

.admin-badge-blue {
  background-color: var(--admin-primary);
}

.admin-badge-teal {
  background-color: var(--admin-accent);
}

.admin-badge-orange {
  background-color: var(--admin-secondary);
}

.admin-badge-gray {
  background-color: #6c757d;
}


